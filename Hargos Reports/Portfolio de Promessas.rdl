      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>Select CART_ID, CART_Descricao From Carteiras Where CONT_ID &lt;&gt; 8</CommandText>
        <DataSourceName>Neo</DataSourceName>
